   "source": [
    "countryCodes = ['US', 'UK', 'CA', 'AU', 'DE', 'SP', 'FR','JP', 'KR', 'TR', 'AE', 'SA', 'BR']\n",
    "industryCodes = [ # insert industry codes here ]
  },
  {
